{
    "name": "jwohlfert23/laravel-tall",
    "description": "A set of blade components for TALL stack modeled after Tailwind UI",
    "keywords": [
        "jwohlfert23",
        "laravel",
        "tall",
        "alpine"
    ],
    "homepage": "https://github.com/jwohlfert23/laravel-tall",
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Wohlfert",
            "email": "jack.wohlfert@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Jwohlfert23\\LaravelTall\\": "src",
            "Jwohlfert23\\LaravelTall\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jwohlfert23\\LaravelTall\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jwohlfert23\\LaravelTall\\BladeTailwindServiceProvider"
            ],
            "aliases": {
                "LaravelTall": "Jwohlfert23\\LaravelTall\\BladeTailwindFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
